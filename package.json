{
  "name": "@vozzen/inisync",
  "version": "1.1.0",
  "description": "A tool to watch incomiung INI files and sync them to a target",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "MIT",
  "keywords": [
    "ini",
    "sync",
    "watch"
  ],
  "scripts": {
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "precompile": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/ini": "^4.1.1",
    "@types/node": "^22.7.5",
    "gts": "^6.0.2",
    "jest": "^29.7.0",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "ini": "^5.0.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0"
  },
  "bin": {
    "inisync": "build/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vozzen/inisync.git"
  },
  "author": "Volkan Sen",
  "bugs": {
    "url": "https://github.com/vozzen/inisync/issues"
  },
  "homepage": "https://github.com/vozzen/inisync#readme",
  "publishConfig": {
    "access": "public"
  }
}
